                                                <div class="table-responsive">
                                                    <table id="order-table" class="table" style="width: fit-content">
                                                        <thead>
                                                            <tr>
                                                                <th>Products</th>
                                                                <th>Date</th>
                                                                <th>Amount</th>
                                                                <th>Order Status</th>
                                                                <th>Payment status</th>
                                                                <th>Actions</th>
                                                            </tr>
                                                        </thead>
                                                        <% if(!orders){%>
                                                            <h3>No orders placed yet.<h3>
                                                                    <%}else{%>
                                                                        <tbody>
                                                                            <% orders.sort((a, b)=> new
                                                                                Date(b.orderDate) - new
                                                                                Date(a.orderDate)); %>
                                                                                <% orders.forEach((order)=> { %>
                                                                                    <tr>
                                                                                        <td
                                                                                            style="border-bottom: 1px black solid;">
                                                                                            <ul>
                                                                                                <%
                                                                                                    order.products.forEach((product)=>
                                                                                                    { %>
                                                                                                    <li>•<%= product.productId.name
                                                                                                            %>
                                                                                                    </li>
                                                                                                    <% }); %>
                                                                                            </ul>
                                                                                        </td>
                                                                                        <td
                                                                                            style="border-bottom: 1px black solid;">
                                                                                            <%= order.orderDate %>
                                                                                        </td>
                                                                                        <td
                                                                                            style="border-bottom: 1px black solid;">
                                                                                            ₹<%= order.totalAmount %>
