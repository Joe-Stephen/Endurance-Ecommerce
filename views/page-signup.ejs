<script>
  const phoneNumberInput = document.getElementById('phoneNumber');

phoneNumberInput.addEventListener('input', function(event) {
  const inputValue = event.target.value;
  const regex = /[eE]/g;

  // Remove any 'e' or 'E' characters from the input value
  const filteredValue = inputValue.replace(regex, '');

  // Update the input value with the filtered value
  event.target.value = filteredValue;
});
</script>
<script>
  const usernameInput = document.getElementById('username');

usernameInput.addEventListener('input', function(event) {
  const inputValue = event.target.value;

  // Validate the username using a regular expression
  if (!/^[a-zA-Z0-9]+$/.test(inputValue)) {
    // Display an error message
    const errorMessage = document.getElementById('errSignupName');
    errorMessage.textContent = 'Username can only contain letters and numbers.';

    // Prevent further input
    event.preventDefault();
  } else {
    // Clear the error message
    const errorMessage = document.getElementById('errSignupName');
    errorMessage.textContent = '';
  }
});

</script>
<script>
document.addEventListener("DOMContentLoaded", function () {
  const sendOTPButton = document.querySelector(".sendOTPButton");

  sendOTPButton.addEventListener("click", function (e) {
    e.preventDefault();

    const username = document.querySelector('input[name="username"]').value;
    const password = document.querySelector('input[name="password"]').value;
    const confirm_password = document.querySelector('input[name="confirm_password"]').value;
    const email = document.querySelector('input[name="email"]').value;
    const phoneNumber = document.querySelector('input[name="phoneNumber"]').value;

    // Check for required fields
    if (!username || !password || !confirm_password || !email || !phoneNumber) {
      // Handle form validation or error messages here
      // For example, you can display an error message or prevent the fetch if any field is missing.
      return;
    }

    const data = {
      username: username,
      password: password,
      confirm_password: confirm_password,
      email: email,
      phoneNumber: phoneNumber
    };

    const queryString = Object.keys(data)
      .map(key => encodeURIComponent(key) + '=' + encodeURIComponent(data[key]))
      .join('&');

    fetch("/send_otp", {
      method: "POST",
      headers: {
        "Content-Type": "application/json"
      },
      body: JSON.stringify(data)
    })
      // .then((response) => {
      //   if (!response.ok) {
      //     throw new Error(`HTTP error! Status: ${response.status}`);
      //   }
      //   return response.json();
      // })
      .then((data) => {
        console.log("Response from server:", data);
        // Redirect to the OTP verification page

        window.location.href =  `/toVerifyPage?${queryString}`;
      })
      .catch((error) => {
        console.error("Error sending OTP:", error);
      });
  });
});
</script>

                     
              

