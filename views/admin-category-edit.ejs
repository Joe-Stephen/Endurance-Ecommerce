        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title text-center">Edit category</h2>
                </div>
            </div>
            <div class="card mb-4 mx-auto" style="max-width: 600px;">
                <header class="card-header">
                </header>
                <div class="card-body">
                    <div class="table-responsive">
        
                        <form action="/admin/edit-category" method="POST" onsubmit="return validateForm()">
                            <div class="mb-4">
                                <input type="hidden" name="categoryId" value="<%= categoryDetails._id %>">
                                <label for="categoryName" class="form-label">Name</label>
                                <input
                                    type="text"
                                    value="<%= categoryDetails.name %>"
                                    name="name"
                                    class="form-control"
                                    id="categoryName"
                                    oninput="validateName(this)"
                                />
                                <span id="nameError" style="color: red;"></span>
                            </div>
                            <div class="mb-4">
                                <label class="form-label">Description</label>
                                <input name="description" value="<%= categoryDetails.description %>" style="width: 100%;" type="text" name="" id="description" oninput="validateDescription(this)">
                                <span id="descriptionError" style="color: red;"></span>
                            </div>
                            <div class="d-grid">
                                <button type="submit" class="btn btn-primary">Save changes</button>
                            </div>
                        </form>
                        
                        <script>
                            function validateName(input) {
                                var name = input.value;
                                // Remove the 'forbiddenChars' variable and related code
                                document.getElementById('nameError').textContent = '';
                            }
                        
                            function validateDescription(input) {
                                var description = input.value;
                                if (description.length < 5) {
                                    document.getElementById('descriptionError').textContent = 'Description must be at least 5 characters long.';
                                } else {
                                    document.getElementById('descriptionError').textContent = '';
                                }
                            }
                        
                            function validateForm() {
                                var name = document.getElementById('categoryName').value;
                                var description = document.getElementById('description').value;
                        
                                // Check for consecutive special characters
                                if (/[\W_]{2,}/.test(name) || /[\W_]{2,}/.test(description)) {
                                    Swal.fire({
                                        icon: 'error',
                                        title: 'Oops...',
                                        text: 'Please avoid consecutive special characters in the fields!',
                                    });
                                    return false; // Prevent form submission
                                }
                        
                                // Your other validation logic here
                                if (name.trim() === '' || description.trim() === '' || description.length < 5) {
                                    Swal.fire({
                                        icon: 'error',
                                        title: 'Oops...',
                                        text: 'Name and description are required fields, and description must be at least 5 characters long.',
                                    });
                                    return false; // Prevent form submission
                                }
                        
                                // Additional form validation checks can be added here
                        
                                return true; // Submit the form if all validations pass
                            }
                        </script>
                        
                        
                                                
