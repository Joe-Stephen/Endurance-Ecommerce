                  <script>

                    function openSizeModal(productId) {
                      var modal = document.getElementById('sizeModal');
                      modal.style.display = 'block';
                      // Set the current product ID in the form
                      document.getElementById('sizeForm').setAttribute('data-product-id', productId);
                    }

                    function closeSizeModal() {
                      var modal = document.getElementById('sizeModal');
                      modal.style.display = 'none';
                    }

                    function addToCartWithSize(event) {
                      event.preventDefault();
                      const productId = document.getElementById('sizeForm').getAttribute('data-product-id');
                      const selectedSize = getSelectedSize();

                      if (selectedSize) {

                        fetch(`/addToCart?selectedSize=${selectedSize}&productId=${productId}`, {
                          method: 'POST',
                          headers: {
                            'Content-Type': 'application/json',
                          },
                          body: JSON.stringify({ productId: productId }),
                        })
                          .then((response) => {

                            if (!response.ok) {
                              throw new Error(`HTTP error! Status: ${response.status}`);
                            }
                            return response.json();
                          })
                          .then((data) => {
                            setTimeout(() => {
                              Swal.fire({
                                position: 'top-center',
                                icon: 'success',
                                title: 'Product added to cart!',
                                showConfirmButton: false,
                                timer: 1500,
                              });
                              closeSizeModal();
                            }, 100);
                            closeSizeModal();
                          })
                          .catch((error) => {
                            window.location.href = '/getLogin';
                          });
                      } else {
                        alert('Please select a size.');
                      }
                    }
                    function getSelectedSize() {
                      const sizeRadios = document.querySelectorAll('#sizeForm input[type="radio"]:checked');
                      return sizeRadios.length > 0 ? sizeRadios[0].value : null;
                    }
                  </script>


                  <script>
                    document.addEventListener("DOMContentLoaded", function () {
                      const addToWishlistButtons = document.querySelectorAll(".addToWishlist");

                      addToWishlistButtons.forEach((button) => {
                        button.addEventListener("click", function (e) {
                          e.preventDefault();

                          const productId = button.getAttribute("data-product-id");

                          fetch("/addToWishlist", {
                            method: "post",
                            headers: {
                              "content-type": "application/json",
                            },
                            body: JSON.stringify({ productId: productId }),
                          })
                            .then((response) => {
                              if (!response.ok) {
                                throw new Error("Product already in the Wishlist");
                              }

                              return response.json();
                            })
                            .then((data) => {

                              Swal.fire({
                                position: 'top-centre',
                                icon: 'success',
                                title: 'Added to Wishlist',
                                showConfirmButton: false,
                                timer: 1500
                              });
                            })
                            .catch((error) => {
                              console.error(error);
                              Swal.fire({
                                icon: "error",
                                title: "Oops...",
                                text: error.message || "Failed to add product to Wishlist",
                                footer: '',
                                timer: 1500
                              });
                            });
                        });
                      });
                    });
